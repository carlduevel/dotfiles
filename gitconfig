[push]
	default = simple
[core]
	excludesfile = /home/carl/.gitignore
	editor = vim
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[diff]
    tool = meld
[difftool]
    prompt = false
[difftool "meld"]
    cmd = meld "$LOCAL" "$REMOTE"
[pull]
	rebase = true
[rebase]
	autoStash = true
